type Bank_type {
	id: ID!
	bank_name: String!
	bank_code: String
}
type Query {
	findAll: [Bank_type]!
}
type Mutation {
	createBank_type(bank_name: String! bank_code: String): Bank_type!
	updateBank_type(id: ID!, bank_name: String!, bank_code: String): Bank_type!
	deleteBank_type(id: ID!): Boolean
}